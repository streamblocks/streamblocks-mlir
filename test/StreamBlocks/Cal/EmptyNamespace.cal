namespace test:

    type T(type A, int b) : A(A X, int(size=b) Y) | B end

    actor Source() ==> int O:

        int Counter := 0;

        action ==> O:[token]
        guard
            Counter < 16
        var
            int token = Counter
        do
            Counter := Counter + 1;
        end

    end

    actor Sink() int I ==> :

        action I:[token] ==>
        do
            println(token);
        end

    end

    network SourceSink() ==> :

        entities
            source = Source();
            sink   = Sink();
        structure
            source.O --> sink.I;
    end


end